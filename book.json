{
    "plugins": [
      "prism", "-highlight",
      "js-console@git+https://github.com/tuanquynet/gitbook-plugin-js-console.git",
      "addcssjs",
      "include-codeblock",
      "ace",
      "disqus",
      "ga"
    ],
    "pluginsConfig": {
        "prism": {
            "css": ["prismjs/themes/prism-okaidia.css"]
        },
        "addcssjs": { },
        "include-codeblock": {
          "template": "ace",
          "unindent": true,
          "theme": "monokai"
        },
        "disqus": {
          "shortName": "learn-datastructure"
        },
        "ga": {
          "token": "UA-159057782-1",
          "configuration": {
              "cookieName": "learn_datastructure",
              "cookieDomain": "learn-datastructure.tuanquynet.click"
          }
      }
    },
    "root": "./docs"
}
